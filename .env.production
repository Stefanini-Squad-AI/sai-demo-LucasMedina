# .env.production
# Configuración para producción (GitHub Pages)

# API Configuration
VITE_API_BASE_URL=/api
VITE_USE_MOCKS=true

# Production features
VITE_ENABLE_DEV_TOOLS=false
VITE_SHOW_TEST_DATA=false
VITE_LOG_LEVEL=error

# Mock configuration (para GitHub Pages)
VITE_MOCK_DELAY_MIN=100
VITE_MOCK_DELAY_MAX=300

# Debug features (deshabilitado en producción)
VITE_ENABLE_REDUX_DEVTOOLS=false
VITE_ENABLE_MSW_LOGGING=false